{
  "name": "@antmjs/doc",
  "version": "2.3.4",
  "description": "PC端在线文档构建工具",
  "main": "index.js",
  "types": "./types/index.d.ts",
  "bin": {
    "antm-doc-build": "./scripts/build.ts",
    "antm-doc-start": "./scripts/run.ts"
  },
  "scripts": {
    "start": "npx ts-node --transpileOnly --transpiler ts-node/transpilers/swc  ./scripts/run",
    "build": "npx ts-node --transpileOnly --transpiler ts-node/transpilers/swc ./scripts/build",
    "release": "npx gh-pages -d ./doc_build -e ./antm-doc"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "document",
    "doc",
    "docs",
    "antmjs"
  ],
  "author": "",
  "repository": {
    "type": "https",
    "url": "https://github.com/AntmJS/antm/tree/main/packages/doc"
  },
  "bugs": {
    "url": "https://github.com/AntmJS/antm/issues/new"
  },
  "engines": {
    "node": ">=16.4",
    "npm": ">=8",
    "yarn": ">=1.22"
  },
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/preset-typescript": "^7.21.4",
    "@swc/core": "^1.3.52",
    "@swc/wasm": "^1.3.52",
    "@types/react-dom": "^18.0.11",
    "ast-to-markdown": "^1.0.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "chokidar": "^3.5.3",
    "classnames": "^2.3.2",
    "css-loader": "^6.7.3",
    "flexsearch": "^0.7.31",
    "glob": "^9.3.4",
    "highlight.js": "^11.7.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "markdown-it": "^13.0.1",
    "markdown-to-ast": "^6.0.3",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss-loader": "^7.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "webpack": "^5.78.0",
    "webpack-dev-server": "^4.13.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1"
  }
}
